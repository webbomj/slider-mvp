{
  "exclude": [
    "src/**/*.spec.*",
    "node_modules",
    "**/__tests__/*",
    "**/__mocks__/*"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.js",
    "test/**/*.ts",
    "src/layers/view/label"
  ],
  "CommonJS": "ES5",
  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,
    "strictFunctionTypes": false
  }
}
